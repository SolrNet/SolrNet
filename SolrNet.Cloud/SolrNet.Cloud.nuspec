    <file src="bin\release\net46\SolrNet.Cloud.xml" target="lib\net46\SolrNet.Cloud.xml" />
    <file src="bin\release\net46\SolrNet.Cloud.pdb" target="lib\net46\SolrNet.Cloud.pdb" />
    <file src="bin\release\netstandard2.0\SolrNet.Cloud.dll" target="lib\netstandard2.0\SolrNet.Cloud.dll" />
    <file src="bin\release\netstandard2.0\SolrNet.Cloud.xml" target="lib\netstandard2.0\SolrNet.Cloud.xml" />
    <file src="bin\release\netstandard2.0\SolrNet.Cloud.pdb" target="lib\netstandard2.0\SolrNet.Cloud.pdb" />
  </files>
</package>